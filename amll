let Nnumber1, Nnumber2, Nnumber3: number;
Number1 = parseInt(prompt('الرجاء ادخال الرقم الاول'));
Nnumber2 = parseInt(prompt('الرجاء ادخال الرقم الثاني'));
Nnumber3 = Number1 * Nnumber2;
if (Nnumber3 > 0) {
  console.log('الرقم موجب');
} else {
  console.log('الرقم سالب');
}
console.log('النهاية ');
